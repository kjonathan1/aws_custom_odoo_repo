<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for eclinic.patient -->
        <record id="seq_patient" model="ir.sequence">
            <field name="name">Patient</field>
            <field name="code">clinic.patient</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for clinic.rdv -->
        <record id="seq_rdv" model="ir.sequence">
            <field name="name">Rendez-vous</field>
            <field name="code">clinic.rdv</field>
            <field name="prefix">RDV</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for clinic.evaluation -->
        <record id="seq_evaluation" model="ir.sequence">
            <field name="name">Evaluation</field>
            <field name="code">clinic.evaluation</field>
            <field name="prefix">CK-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for clinic.factureass -->
        <record id="seq_factureass" model="ir.sequence">
            <field name="name">Factureass</field>
            <field name="code">clinic.factureass</field>
            <field name="prefix">FACTASS-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

         <!-- Sequences for clinic.factureasspaiement -->
        <record id="seq_paiementass" model="ir.sequence">
            <field name="name">PAIEMENT ASSURANCE</field>
            <field name="code">clinic.factureasspaiement</field>
            <field name="prefix">PAIEASS-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</odoo>
